{
    "llvm-target": "x86_64-unknown-linux-gnu",
    "data-layout": "e-m:e-i64:64-f80:128-n8:16:32:64-S128",
    "arch": "x86_64",
    "target-endian": "little",
    "target-pointer-width": "64",
    "target-c-int-width": "32",
    "executables": true,
    "linker-flavor": "gcc",
    "linker": "gcc",
    "pre-link-args": {
        "gcc": [
            "-Wl,--script=link-script",
            "-Wl,--omagic",
            "-s",
            "-static",
            "-nostartfiles"
        ]
    },
    "panic-strategy": "abort",
    "os": "linux"
}
